{
  "contextPath": "applitutoriel-js",
  "fullSpa": {
    "enabled": true,
    "host": "http://localhost/",
    "name": "applitutoriel-service",
    "staticPath": "/static"
  },
  "services": {
    "host": "http://localhost/",
    "name": "applitutoriel-service"
  },
  "themeName": "hornet-themes-intranet",
  "localeI18n": {
    "locale": "fr-FR",
    "lang": "fr"
  },
  "logClient": {
    "remote": false,
    "level": "DEBUG",
    "appenders": [
      {
        "type": "BrowserConsole",
        "layout": {
          "type": "THIN"
        }
      },
      {
        "type": "Ajax",
        "layout": {
          "type": "BASIC"
        },
        "threshold": 100,
        "timeout": 3000,
        "url": "/log"
      }
    ]
  },
  "themeUrl": "/applitutoriel-js/static/hornet-themes-intranet"
}
